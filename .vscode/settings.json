{

"cSpell.words": [
  "swconcord",
  "adoc"
  ],

"cSpell.language": "en-US,la,sv,en-GB",

"cSpell.customDictionaries": {
    "swedenborgEnglish": {
      "name": "Swedenborg English words",
      "path": "${workspaceFolder}/../swedenborg-dictionaries/.dictionaries/dictionary-en-swedenborg.txt",
      "scope": "workspace",
      "addWords": true
    },
    "swedenborgWorksAbbrevs": {
      "name": "Swedenborg Works abbreviations",
      "path": "${workspaceFolder}/../swedenborg-dictionaries/.dictionaries/dictionary-works-abbrev.txt",
      "scope": "workspace",
      "addWords": false
    },
    "swedenborgLatin": {
      "name": "Swedenborg Latin words",
      "path": "${workspaceFolder}/../swedenborg-dictionaries/.dictionaries/dictionary-la-swedenborg.txt",
      "scope": "workspace",
      "addWords": true
    },
    "Swedish": {
      "name": "Swedish dictionary",
      "path": "${workspaceFolder}/../swedenborg-dictionaries/.dictionaries/dictionary-sv.txt",
      "scope": "workspace",
      "addWords": false
    },
    "Latin": {
      "name": "Latin dictionary provided by Lewis and Short (do not modify)",
      "path": "${workspaceFolder}/../swedenborg-dictionaries/.dictionaries/dictionary-la-lewis-short.txt",
      "scope": "workspace",
      "addWords": false
    },
    "concordance-words": {
      "name": "concordance-words",
      "path": "${workspaceFolder}/../swedenborg-dictionaries/.dictionaries/concordance-words.txt",
      "description": "Words used in this project",
      "scope": "workspace",
      "addWords": false
    },
    "proper-names": {
      "name": "proper-names",
      "path": "${workspaceFolder}/../swedenborg-dictionaries/.dictionaries/proper-names.txt",
      "description": "Proper Names",
      "scope": "workspace",
      "addWords": true
    },
    "old-style": {
      "name": "old-style",
      "path": "${workspaceFolder}/../swedenborg-dictionaries/.dictionaries/old-style.txt",
      "description": "Old Style [...(est|eth)]",
      "scope": "workspace",
      "addWords": true
    },
   "project-words": {
      "name": "project-words",
      "path": "${workspaceFolder}/../swedenborg-dictionaries/.dictionaries/project-words.txt",
      "description": "Words used in this project",
      "addWords": true
    }
  },

"cSpell.minWordLength": 2,

"cSpell.enabledFileTypes": {
  "asciidoc": true,
  "txt": true
},


// Always assume first line of CSV file is the header
"csv-edit.readOption_hasHeader": "true",

// Don't show distracting git diffs in overview ruler
// "scm.diffDecorations": "none",

"asciidoc.preview.asciidoctorAttributes": {
  },
  "cSpell.showAutocompleteDirectiveSuggestions": false,


}

